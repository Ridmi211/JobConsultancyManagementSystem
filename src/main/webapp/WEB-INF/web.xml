<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AppointmentController</servlet-name>
  	<display-name>AppointmentController</display-name>
  	<description></description>
  	<servlet-class>com.jobConsultancyScheduler.controller.AppointmentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterController</servlet-name>
  	<display-name>RegisterController</display-name>
  	<description></description>
  	<servlet-class>com.jobConsultancyScheduler.controller.RegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.jobConsultancyScheduler.controller.UserController</servlet-class>
  </servlet>
   <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/appointmentManager</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/getAppointment</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>UserController</servlet-name>
   	<url-pattern>/usermanager</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>UserController</servlet-name>
   	<url-pattern>/getuser</url-pattern>
   </servlet-mapping>
   

</web-app>
